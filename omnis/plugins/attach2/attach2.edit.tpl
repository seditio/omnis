<!-- BEGIN: MAIN -->
<ul id="attachments" class="list-unstyled mb-0 d-grid gap-3">
<!-- BEGIN: ATTACH_ROW -->
	<li>
		<blockquote class="mx-3 my-2" title="{PHP.L.theme-click-to-copy}" data-domain="{PHP.cfg.mainurl}/">{ATTACH_ROW_ID|att_thumb($this, 966, 644, 'crop')}</blockquote>
		<a href="{ATTACH_ROW_URL}" class="fancybox" rel="gallery" title="{ATTACH_ROW_TITLE}" >
			<img src="{ATTACH_ROW_ID|att_thumb($this, 648, 432, 'crop')}" alt="{ATTACH_ROW_FILENAME}" class="img-fluid" />
		</a>
	</li>
<!-- END: ATTACH_ROW -->
</ul>
<!-- END: MAIN -->
